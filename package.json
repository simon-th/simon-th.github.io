{
  "homepage": "https://simon-th.github.io/",
  "name": "gatsby-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Gatsby Starter",
  "author": "Simon Hoque",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "npx gatsby develop",
    "start": "npx gatsby develop",
    "build": "npm run type-check && npm run format && npm run lint && npx gatsby build --prefix-paths",
    "serve": "npx gatsby serve",
    "clean": "npx gatsby clean",
    "type-check": "npx tsc --noEmit --skipLibCheck",
    "format": "npx prettier --check .",
    "format-fix": "npx prettier --write .",
    "lint": "npx eslint .",
    "lint-fix": "npx eslint --fix .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public"
  },
  "dependencies": {
    "@fontsource/public-sans": "^4.5.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "gatsby": "^4.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gatsby-plugin-postcss": "^5.4.0",
    "gh-pages": "^3.2.3",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.10",
    "typescript": "^4.5.4"
  }
}
